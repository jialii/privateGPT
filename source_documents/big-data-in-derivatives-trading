function truncateString(str, num) {
  if (str.length <= num) {
    return str
  }
  return str.slice(0, num) + '...'
}
 
let x = document.getElementById("toBeTruncated").innerHTML;
 
document.getElementById("toBeTruncated").innerHTML = truncateString(x, 25);
