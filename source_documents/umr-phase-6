<script>
// This is for the timeline

  (function() {
    var idGenerator = function() {
      return 'timeline-modal-' + Math.random().toString(36).substr(2, 9);
    };
    $('.timeline-scroller-item').each(function(index, element) {
      var $element = $(element);
      var id = idGenerator();
      $element.find('.text-button-timeline').attr('data-target', '#' + id);
      $element.find('.modal').attr('id', id);
    });
    $(document).on('click', '.timeline-scroller-item .text-button-timeline', function(e) {
      e.preventDefault();
      var modalTarget = $(this).attr('data-target');
      $(modalTarget).modal({
        fadeDuration: 250
      });
    });
  })();
  (function() {
    $('.timeline-scroller').slick({
      infinite: false,
      slidesToShow: 5,
      slidesToScroll: 1,
      swipeToSlide: true,
      initialSlide: 4,
      arrows: false,
      centerMode: true,
      responsive: [
        {
          breakpoint: 1830,
          settings: {
            slidesToShow: 4,
            initialSlide: 4
          }
        },
        {
          breakpoint: 1480,
          settings: {
            slidesToShow: 3,
            initialSlide: 4
          }
        },
        {
          breakpoint: 1100,
          settings: {
            slidesToShow: 2,
            initialSlide: 3
          }
        },
        {
          breakpoint: 750,
          settings: {
            slidesToShow: 1,
            initialSlide: 4
          }
        }
      ]
    });
  })();
  (function() {
    var one_day = 1000 * 60 * 60 * 24;
    document.querySelectorAll('.timeline-event-circle, .timeline-event-status').forEach(function(element) {
      var today = new Date();
      var targetDate = new Date(element.textContent);
      var daysLeft = Math.ceil((targetDate.getTime() - today.getTime()) / one_day);
      if (daysLeft > 0) {
        var text = daysLeft === 1 ? "day left" : "days left";
        element.innerHTML = "<span>" + daysLeft + "</span> " + text;
        //element.parentNode.querySelector('.timeline-event-status').innerHTML = daysLeft + " " + text;
        element.classList.add('timeline-event-circle-date');
      } else (element.innerHTML = "Should be completed")
    });
  })();
</script>

<script>
  // This is for the countdown timer
  
  var countDownDate = new Date("Aug 31, 2022 23:59:59").getTime();

var x = setInterval(function() {
  
  var now = new Date().getTime();
    
  var distance = countDownDate - now;
    
  var days = Math.floor(distance / (1000 * 60 * 60 * 24));
  var hours = Math.floor((distance % (1000 * 60 * 60 * 24)) / (1000 * 60 * 60));
  var minutes = Math.floor((distance % (1000 * 60 * 60)) / (1000 * 60));
  var seconds = Math.floor((distance % (1000 * 60)) / 1000);

  document.getElementById("days").innerHTML = days
  document.getElementById("hours").innerHTML = hours
  document.getElementById("mins").innerHTML = minutes
  document.getElementById("secs").innerHTML = seconds

  if (distance < 0) {
    clearInterval(x);
    document.getElementById("demo").innerHTML = "EXPIRED";
  }
}, 1000);
</script></body></html>